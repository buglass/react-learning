  "scripts": {
    "test": "jest",
    "test-cov": "jest --coverage",
    "serve": "webpack-dev-server --open --config ./config/webpack/webpack.config.dev.js",
    "serve-single-spa": "npm run build-single-spa && npm run copy-single-spa-output && npm run serve",
    "copy-single-spa-output": "copyfiles -u 3 ./build/dist/app/main.js ./public --verbose",
    "build": "npm run build-dev",
    "build-dev": "webpack --config ./config/webpack/webpack.config.dev.js",
    "build-prod": "webpack --config ./config/webpack/webpack.config.prod.js",
    "build-single-spa": "webpack --config ./config/webpack/webpack.config.single-spa.js",
    "lint": "eslint --ext ts,tsx,json ./ --format html -o build/reports/lint/eslint_report.html",
    "lint-fix": "npm run lint -- --fix",
    "lint-scss": "stylelint --custom-formatter ./config/stylelint/stylelint-html-reporter.js **/*.scss",
    "lint-markdown": "markdownlint *.md ./**/*.md ./**/**/*.md --ignore node_modules",
    "lint-spelling": "mdspell -rna *.md ./docs/*.md ./docs/**/*.md ./infrastructure/*.md !**/node_modules/**/*.md",
    "vet": "npm run lint && npm run lint-scss && npm run lint-markdown && npm run lint-spelling",
    "sonar": "sonar-scanner -Dproject.settings=sonar-project.properties -Dsonar.login=$(SONAR_TOKEN) -Dsonar.branch.name=${CODEBUILD_WEBHOOK_HEAD_REF}",
    "sonar-master": "sonar-scanner -Dproject.settings=sonar-project.properties -Dsonar.login=$(SONAR_TOKEN)"
  },
